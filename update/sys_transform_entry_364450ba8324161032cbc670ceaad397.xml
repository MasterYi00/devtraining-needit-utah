        <source_script><![CDATA[answer = (function transformEntry(source) {

	// Add your code here
	  var sourceDate = source.u_when_needed;
    var dateParts = sourceDate.split("/");

    // Prüfen, ob das Datum im erwarteten Format vorliegt
    if (dateParts.length === 3) {
        var month = dateParts[0];
        var day = dateParts[1];
        var year = dateParts[2];

        // Jahr formatieren, wenn es zweistellig ist
        if (year.length === 2) {
            year = "20" + year;
        }

        // Datum in das gewünschte Format umwandeln
        var formattedDate = year + "-" + month + "-" + day + " 00:00:00";
        return formattedDate;
    } else {
        // Fehlerbehandlung: Datum liegt nicht im erwarteten Format vor
        gs.error("Datum im falschen Format: " + sourceDate);
        return null;
    }

})(source);]]></source_script>
